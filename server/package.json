    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "axios": "^1.7.5",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.4.0",
    "config": "^3.3.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^6.1.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "express-rate-limit": "^6.7.0",
    "faker": "^4.1.0",
    "fcm-node": "^1.6.1",
    "file-get-contents": "^1.0.0",
    "forever": "^0.14.2",
    "joi": "^14.3.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mailgun.js": "^8.0.6",
    "moment": "^2.29.3",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.1",
